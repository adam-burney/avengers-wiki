{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {
        "react-hooks/rules-of-hooks": "error"  // Vérifie les règles des Hooks
        //"react-hooks/exhaustive-deps": "warn"  // Vérifie les tableaux de dépendances **Waring Timestamp.js
      }
}
